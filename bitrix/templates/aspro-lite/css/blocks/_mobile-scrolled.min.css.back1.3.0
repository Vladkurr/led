@media (max-width:600px){.mobile-scrolled{display:flex;flex-wrap:nowrap;overflow:auto;padding-bottom:15px!important;margin-bottom:-15px!important;padding-top:15px!important;margin-top:-15px!important;grid-gap:0}#main .mobile-scrolled:not(.grid-list--no-gap)>div{padding:0 8px 0 0}#main .mobile-scrolled>div{width:420px;flex-shrink:0;box-sizing:content-box;order:unset}#main .mobile-scrolled--items-2>div{width:260px}#main .mobile-scrolled--items-3>div{width:164px}#main .mobile-scrolled.mobile-scrolled--items-auto>div{width:auto;padding:0}#main .mobile-scrolled.mobile-scrolled--small-offset>div{padding-right:calc(var(--theme-page-width-padding)/ 2)}#main .mobile-scrolled.mobile-scrolled--small-offset>div:last-of-type{padding-right:var(--theme-page-width-padding)}#main .mobile-offset{margin:0 calc(-1 * var(--theme-page-width-padding));padding:0 0 0 var(--theme-page-width-padding);flex-wrap:nowrap}#main .mobile-offset--right{padding:0 var(--theme-page-width-padding)}}@media (max-width:450px){.mobile-scrolled>div{width:320px}}